{
  "name": "grachevko/enum",
  "description": "Enum class.",
  "license": "MIT",
  "authors": [
    {
      "name": "Konstantin Grachev",
      "email": "me@grachevko.ru"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "Grachevko\\Enum\\": "src/",
    },
    "exclude-from-classmap": ["/tests/"]
  },
  "autoload-dev": {
      "psr-4": {
          "Grachevko\\Enum\\Tests\\": "tests/"
      }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.2.1",
    "friendsofphp/php-cs-fixer": "^2.12.1",
    "phpstan/phpstan": "^0.11.2"
  }
}
